using System;
using System.Collections;

class NameSorter
{
    static void Main()
    {
        ArrayList names = new ArrayList();

        for (int i = 0; i < 10; i++)
        {
            Console.Write("Enter name " + (i + 1) + " of 10: ");
            string name = Console.ReadLine();
            names.Add(name);
        }

        names.Sort();

        Console.WriteLine("\nSorted Names:");
        foreach (string name in names)
        {
            Console.WriteLine(name);
        }
    }
}